# EKYC Web Application - .env.example
# Copy this file to .env and fill in the values. Restart the dev server after changes.

# Required Supabase configuration
REACT_APP_SUPABASE_URL=https://YOUR_PROJECT_ID.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-anon-public-key

# Frontend site URL used for auth email redirects (e.g., http://localhost:3000)
REACT_APP_FRONTEND_URL=http://localhost:3000

# Optional/Reserved variables (for future backend integrations or deployment tuning)
REACT_APP_API_BASE=
REACT_APP_BACKEND_URL=
REACT_APP_WS_URL=

# Environment and build behavior
REACT_APP_NODE_ENV=development
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_NEXT_TELEMETRY_DISABLED=true

# Local dev server
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false

# Logging and health checks
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/healthz

# Feature flags
# Example JSON: {"newUploads":"on","betaBanner":true}
# or CSV: newUploads:on,betaBanner:true
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=false
